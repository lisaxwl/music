/**index.wxss**/
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 200px;
}

.miusc_cover {
  height: 300rpx;
  width: 100%;
}
.items {
  display: flex;
  display: -webkit-flex;
  flex-direction:row;
  justify-content:space-between;
  margin:20rpx 42rpx 40rpx;
}

.circle_box {
  font-size: 24rpx;
  text-align: center;
}
.circle {
  width: 100rpx;
  height: 100rpx;
  background-color: #e7473b;
  box-shadow: #e7473b 0 2px 5px 0;
  display: block;
  border-radius:50%;
  margin-bottom: 22rpx;
  display: flex;
  justify-content:center;
  align-items:center
}
.bg {
  width: 50rpx;
  height: 50rpx;
  display: block;
}
.bg1 {
  background: url("data:image/png;base64,");
  background-position: center center;
  background-size:50rpx 50rpx;
}
.bg2 {
  background: url("data:image/png;base64,");
  background-position: center center;
  background-size:50rpx 50rpx;
}
.bg3 {
  background: url("data:image/png;base64,");
  background-position: center center;
  background-size:50rpx 50rpx;
}
.bg4 {
  background: url("data:image/png;base64,");
  background-position: center center;
  background-size:50rpx 50rpx;
}

.line {
  border-bottom: 1px solid #e5e9e8;
}
.music_list{
   margin-top: 50rpx;
   margin-bottom: 50rpx;
}
.title .icon-youbianjiantou {
  font-size:24rpx;
}
.title {
  font-size: 24rpx;
  font-weight: bold;
  margin-left: 12rpx;
  height: 32rpx;
  line-height: 32rpx;
}
.flex_box {
  padding: 0 10rpx;
  display: flex;
  flex-direction: row;
  flex-wrap:wrap;
  justify-content:center;
}
.list_data {
  width: 239rpx;
  height: 306rpx;
  margin-right: 6rpx;
  margin-top: 48rpx;
  position: relative;
}
.list_data:nth-child(3n) {
  margin-right: 0;
}
.data_img {
  width: 239rpx;
  height: 239rpx;
  border-radius: 10rpx;
}
.song_name {
  font-size: 20rpx;
  height: 67rpx;
  display: block;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  max-height: 300%;
  white-space: normal;
}
.listen-icon {
  position: absolute;
  right: 10rpx;
  top: 10rpx;
  font-size: 24rpx;
  color: #ffffff;
}
.listen_small_icon {
  width: 22rpx;
  height: 22rpx;
  margin-right: 6rpx;
}
.play_box {
  height: 100rpx;
  width: 100%;
  padding: 0 10rpx;
  position:fixed;
  bottom:0;
  background-color:rgba(255,255,255,0.9);
  display: flex;
}
.play_cover_img {
  width: 78rpx;
  height: 78rpx;
  border-radius: 5rpx;
  padding:10rpx 0;
  flex-grow:0;
}
.cover_img {
  width: 100%;
  height: 100%;
}

.new_list {
   margin-bottom: 115rpx;
}
audio{
  position: relative;
  display:block;
}
audio .free-MusicProgress{
  position: absolute;
  width:78%;
  left:21.7%;
  bottom:1px;
  background:#C3C3C3;
}
audio .free-MusicProgress>view{
  background:#48c23d;
  height:2px;
}
.fixed {
  height: 100rpx;
  width: 100%;
  position: fixed;
  bottom: 40rpx
}


.swiper-tab{ 
  width: 100%; 
  text-align: center; 
  line-height: 80rpx;
  background: #e7473b;

} 
.tab {
  width: 60rpx;
  height: 60rpx;
}
.swiper-tab-list{ 
  font-size: 30rpx; 
  display: inline-block; 
  width: 33%; 
  color: #FFFFFF; 
  position: relative;
} 
.on{ 
  position: absolute;
  width: 60rpx;
  bottom: 10rpx;
  border-bottom: 5rpx solid #FFFFFF;
  left:50%;
  margin-left: -30rpx;
} 
  
swiper-item  {
  overflow: auto;

}